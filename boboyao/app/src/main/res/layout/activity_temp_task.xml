<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_temp_task"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/divider_list"
    android:orientation="vertical"
    tools:context="com.xinyi.boboyao.activity.TempTaskActivity">


    <include layout="@layout/activity_back_title_bar" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recyleView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"></android.support.v7.widget.RecyclerView>

</LinearLayout>
